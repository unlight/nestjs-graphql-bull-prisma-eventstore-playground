    "@swc/core": "^1.10.0",
    "@swc/helpers": "^0.5.15",
    "@swc/register": "^0.1.10",
    "@trivago/prettier-plugin-sort-imports": "^5.1.0",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.13",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@types/webpack-node-externals": "^3.0.4",
    "await-to-js": "^3.0.0",
    "eslint": "^9.16.0",
