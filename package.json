{
  "name": "nestjs-graphql-bull-prisma-eventstore-playground",
  "private": true,
  "version": "0.0.0-dev",
  "main": "index.js",
  "workspaces": [
    "src/app_modules/*"
  ],
  "scripts": {
    "dev": "node-dev src/main.ts",
    "build": "vite build --ssr --mode=production",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "eslint": "eslint src/**/*.ts",
    "test": "npm run eslint && npm run test:r",
    "test:e2e": "npm run mocha -- \"**/*.e2e-spec.ts\" --no-timeouts --watch-files src",
    "test:r": "npm run mocha -- \"**/*.spec.ts\" --no-timeouts",
    "test:w": "npm run test:r -- -w",
    "mocha": "node -r ts-node/register node_modules/mocha/bin/mocha",
    "prisma:g": "prisma generate",
    "postinstall": "patch-package"
  },
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@eventstore/db-client": "^6.1.0",
    "@nestjs-steroids/environment": "^1.0.2",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.8",
    "@paljs/plugins": "^6.0.7",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.13.0",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.1",
    "class-validator-flat-formatter": "^4.0.2",
    "dotenv": "^16.4.5",
    "gql.tada": "^1.7.0",
    "graphql": "^16.8.1",
    "graphql-apollo-errors": "^2.0.3",
    "graphql-subscriptions": "^2.0.0",
    "nestjs-cqrx": "^2.3.0",
    "nestolog": "^3.0.0",
    "prisma-query-log": "^3.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "serialize-error": "8",
    "typeid-js": "^0.7.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.3.8",
    "@nestjs/websockets": "^10.3.8",
    "@swc/core": "^1.5.5",
    "@swc/helpers": "^0.5.11",
    "@swc/register": "^0.1.10",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "await-to-js": "^3.0.0",
    "expect": "^29.7.0",
    "instant-mocha": "^1.5.2",
    "mocha": "^10.4.0",
    "node-dev": "^8.0.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "simplytyped": "^3.3.0",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
