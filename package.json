{
  "name": "nestjs-graphql-bull-prisma-eventstore-playground",
  "private": true,
  "version": "0.0.0-dev",
  "main": "index.js",
  "workspaces": [
    "src/app_modules/*"
  ],
  "scripts": {
    "dev": "node-dev src/main.ts",
    "devw": "webpack --watch",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "eslint": "eslint src/**/*.ts",
    "mocha": "node -r ts-node/register node_modules/mocha/bin/mocha",
    "prisma:g": "prisma generate",
    "postinstall": "patch-package",
    "graphqlschema:g": "ts-node scripts/generate-graphql-schema.ts",
    "graphqlenv:g": "ts-node scripts/generate-graphql-env.ts",
    "test": "npm run eslint && npm run test:r",
    "test:r": "instant-mocha \"src/**/*.spec.ts\" --webpack-config=webpack.config.cjs -R spec",
    "test:w": "npm run test:r -- --watch",
    "test:e2e": "instant-mocha \"**/*.e2e-spec.ts\" --webpack-config=webpack.config.cjs -R spec --no-timeouts --watch-files src"
  },
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@eventstore/db-client": "^6.2.1",
