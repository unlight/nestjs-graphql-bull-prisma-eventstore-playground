{
  "name": "nestjs-graphql-bull-prisma-eventstore-playground",
  "private": true,
  "version": "0.0.0-dev",
  "main": "index.js",
  "workspaces": [
    "src/app_modules/*"
  ],
  "scripts": {
    "dev": "node-dev src/main.ts",
    "build": "vite build --ssr --mode=production",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "eslint": "eslint src/**/*.ts",
    "test": "npm run eslint && npm run test:r",
    "test:e2e": "npm run mocha -- \"**/*.e2e-spec.ts\" --no-timeouts --watch-files src",
    "test:r": "npm run mocha -- \"**/*.spec.ts\" --no-timeouts",
    "test:w": "npm run test:r -- --watch",
    "mocha": "node -r ts-node/register node_modules/mocha/bin/mocha",
    "prisma:g": "prisma generate",
    "postinstall": "patch-package",
    "graphqlschema:g": "ts-node scripts/generate-graphql-schema.ts",
    "graphqlenv:g": "ts-node scripts/generate-graphql-env.ts"
  },
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@eventstore/db-client": "^6.2.1",
    "@nestjs-steroids/environment": "^1.0.2",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-express": "^10.4.1",
    "@paljs/plugins": "^7.0.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.18.0",
    "bull": "^4.16.0",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.1",
    "class-validator-flat-formatter": "^4.0.2",
    "dotenv": "^16.4.5",
    "gql.tada": "^1.8.6",
    "graphql": "^16.9.0",
    "graphql-subscriptions": "^2.0.0",
    "nestjs-cqrx": "^3.0.1",
    "nestolog": "^3.0.0",
    "prisma-query-log": "^3.2.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0",
    "typeid-js": "^1.0.0"
  },
  "devDependencies": {
    "@bull-board/api": "^5.21.4",
    "@bull-board/express": "^5.21.4",
    "@bull-board/nestjs": "^5.21.4",
    "@eslint/js": "^9.9.1",
    "@nestjs/testing": "^10.4.1",
    "@nestjs/websockets": "^10.4.1",
    "@swc/core": "^1.7.18",
    "@swc/helpers": "^0.5.12",
    "@swc/register": "^0.1.10",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.0",
    "@types/supertest": "^6.0.2",
    "await-to-js": "^3.0.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^3.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^2.0.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "expect": "^29.7.0",
    "globals": "^15.9.0",
    "instant-mocha": "^1.5.2",
    "mocha": "^10.7.3",
    "neverthrow": "^7.0.1",
    "node-dev": "^8.0.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "simplytyped": "^3.3.0",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.25.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "webpack": "^5.94.0",
    "webpack-node-externals": "^3.0.0",
    "workspace-alias": "^0.1.0"
  }
}
